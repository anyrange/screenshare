{
  "name": "screen-share",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "peer": "peerjs --port 9000 --key peerjs --path /peer",
    "dev": "concurrently \"vite\" \"pnpm run peer\"",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 3005",
    "lint": "eslint --ext .ts,vue --ignore-path .gitignore . --fix",
    "format": "prettier -w -u ."
  },
  "dependencies": {
    "@vueuse/core": "^7.5.3",
    "maska": "^1.5.0",
    "nanoid": "^3.2.0",
    "peer": "^0.6.1",
    "peerjs": "^1.3.2",
    "vue": "^3.2.27",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "concurrently": "^7.0.0",
    "eslint": "^7.27.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.10.0",
    "prettier": "^2.3.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-windicss": "^1.6.2",
    "vue-tsc": "^0.29.8",
    "windicss": "^3.4.2"
  }
}