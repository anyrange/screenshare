{
  "name": "screen-share",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "peer": "peerjs --port 9000 --key peerjs --path /peer",
    "dev": "concurrently \"vite\" \"pnpm run peer\"",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "nanoid": "^3.1.30",
    "peer": "^0.6.1",
    "peerjs": "^1.3.2",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.0.0",
    "concurrently": "^7.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  }
}